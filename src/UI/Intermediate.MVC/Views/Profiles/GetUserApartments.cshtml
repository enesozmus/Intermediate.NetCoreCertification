@model IReadOnlyList<GetUserApartmentsQueryResponse>

@{
     ViewData["Title"] = "GetUserApartments";
     Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table table-striped table-hover">
     <thead class="thead-light">
          <tr>
               <th>Blok Adı</th>
               <th>Bulunduğu Kat</th>
               <th>Kapı Numarası</th>
               <th>Daire Tipi</th>
               <th></th>
          </tr>
     </thead>

     <tbody>

          @foreach (var item in Model)
          {
               <tr>
                    <td>@item.BlockName</td>
                    <td>@item.Floor</td>
                    <td>@item.DoorNumber</td>
                    <td>@item.ApartmentType</td>
               </tr>
          }
     </tbody>
</table>

